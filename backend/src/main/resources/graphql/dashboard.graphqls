type DashboardActivity{
    menu:String
    period:String
    totalPending:Int
    totalInProgress:Int
    totalResolved:Int
    totalDropped:Int
    totalTickets:Int
    label:[String]
    data:[Int]
}

type DashboardAnalytics{
    period:String
    minTickets:Int
    maxTickets:Int
    avgTickets:Int
    totalTickets:Int
    label:[String]
    data:[Int]
}

extend type Query{
    getDashboardActivity:[DashboardActivity]
    getDashboardAnalytics:[DashboardAnalytics]
}
type Accounts{
    id:ID!
    username:String
    fullname:String
    email:String
    contactNo:String
    division:Divisions
    profileImg:String
    isActive:Boolean
    roles:[Roles]
}

extend type Query{
    getAllAccounts:[Accounts]
    getAllDevelopers:[Accounts]
    getAccount(accountId:ID!):Accounts
}
